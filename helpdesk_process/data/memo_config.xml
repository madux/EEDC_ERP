<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="memo_stage_helpdesk_adjustment_process_1" model="memo.stage">
            <field name="name">Initiation</field>
            <field name="loaded_from_data">1</field>
            <field name="active">1</field>
            <field name="publish_on_dashboard">1</field>
            <field name="sequence">20</field>
        </record>

        <record id="memo_stage_helpdesk_adjustment_process_2" model="memo.stage">
            <field name="name">To Approve</field>
            <field name="loaded_from_data">1</field>
            <field name="active">1</field>
            <field name="publish_on_dashboard">1</field>
            <field name="sequence">30</field>
        </record>

        <record id="memo_stage_helpdesk_adjustment_process_3" model="memo.stage">
            <field name="name">Completed</field>
            <field name="loaded_from_data">1</field>
            <field name="active">1</field>
            <field name="publish_on_dashboard">1</field>
            <field name="sequence">50</field>
        </record>

        <record id="memo_config_helpdesk_adjustment_process" model="memo.config">
            <field name="memo_type" ref="helpdesk_process.mtype_helpdesk"/>
            <field name="active">1</field>
            <field name="name">METER ADJUSTMENT</field>
            <field name="department_id" ref="hr.dep_administration"/>
            <field name="stage_ids" eval="[
            (4, ref('helpdesk_process.memo_stage_helpdesk_adjustment_process_1')),
            (4, ref('helpdesk_process.memo_stage_helpdesk_adjustment_process_2')),
            (4, ref('helpdesk_process.memo_stage_helpdesk_adjustment_process_3')),
            ]"/>
        </record>

        <record id="helpdesk_process.memo_stage_helpdesk_adjustment_process_1" model="memo.stage">
            <field name="memo_config_id" ref="helpdesk_process.memo_config_helpdesk_adjustment_process"/>
        </record>

        <!-- ################## BILLING ON DISCONNECTION STAGE ###################  -->
        <record id="memo_stage_helpdesk_bod_process_1" model="memo.stage">
            <field name="name">Initiation</field>
            <field name="loaded_from_data">1</field>
            <field name="active">1</field>
            <field name="publish_on_dashboard">1</field>
            <field name="sequence">20</field>
        </record>
        <record id="memo_stage_helpdesk_bod_process_2" model="memo.stage">
            <field name="name"> Billing Manager To Approve</field>
            <field name="loaded_from_data">1</field>
            <field name="active">1</field>
            <field name="publish_on_dashboard">1</field>
            <field name="sequence">20</field>
        </record>
        <record id="memo_stage_helpdesk_bod_process_3" model="memo.stage">
            <field name="name">Customer Care Representative to Close ticket</field>
            <field name="loaded_from_data">1</field>
            <field name="active">1</field>
            <field name="publish_on_dashboard">1</field>
            <field name="sequence">30</field>
        </record>
        <record id="memo_stage_helpdesk_bod_process_4" model="memo.stage">
            <field name="name">Completed</field>
            <field name="loaded_from_data">1</field>
            <field name="active">1</field>
            <field name="publish_on_dashboard">1</field>
            <field name="sequence">50</field>
        </record>
        <record id="memo_config_helpdesk_bod_process" model="memo.config">
            <field name="memo_type" ref="helpdesk_process.mtype_helpdesk"/>
            <field name="active">1</field>
            <field name="name">BILLING ON DISCONNECTION</field>
            <field name="department_id" ref="hr.dep_administration"/>
            <field name="stage_ids" eval="[
            (4, ref('helpdesk_process.memo_stage_helpdesk_bod_process_1')),
            (4, ref('helpdesk_process.memo_stage_helpdesk_bod_process_2')),
            (4, ref('helpdesk_process.memo_stage_helpdesk_bod_process_3')),
            (4, ref('helpdesk_process.memo_stage_helpdesk_bod_process_4')),
            ]"/>
        </record>
        
    </data>
</odoo>
