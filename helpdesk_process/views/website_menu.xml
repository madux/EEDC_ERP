<odoo>
    <data noupdate="0">

        <!-- Website menu item pointing to your custom ticket page -->
        <record id="website_menu_customer_ticket" model="website.menu">
            <field name="name">Create Ticket</field>
            <field name="url">http://localhost:8070/customerTicket</field>
            <field name="sequence" type="int">50</field>
            <field name="website_id" ref="website.default_website"/>
            <!-- Optional: Make it a top-level item or set parent -->
            <!-- <field name="parent_id" ref="website.main_menu"/> -->
        </record>

    </data>
</odoo>