<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="economic_tag_view_tree" model="ir.ui.view">
			<field name="name">economic.tag.view.tree</field>
			<field name="model">economic.tag</field>
			<field name="arch" type="xml">
				<tree string="">
					<field name="name"/>
					<field name="parent_tag_id" /> 
					<field name="account_head_type"/>
				</tree>
			</field>
		</record>

		<record id="economic_tag_view_form" model="ir.ui.view">
			<field name="name">economic.tag.view.form</field>
			<field name="model">economic.tag</field>
			<field name="arch" type="xml">
				<form string="">
					<sheet>
						<group>
							<group>
								<field name="name"/>
								<field name="code" />
								<field name="parent_tag_id" /> 
							</group>
							<group>
								<field name="account_head_type"/>
								<field name="account_ids" widget="many2many_tags"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="economic_tag_action" model="ir.actions.act_window">
			<field name="name">Economic Tag</field>
			<field name="res_model">economic.tag</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[]</field>
			<field name="context">{'group_by': 'account_head_type'}</field>
			<field name="help" type="html">
				<p> Click New to create </p>
			</field>
		</record>

		<menuitem
			id="economic_tag_menu"
			name="Economic Tag"
			action="economic_tag_action"
			parent="eedc_report.eedc_report_main_menu"
			sequence="30"/>
</odoo>