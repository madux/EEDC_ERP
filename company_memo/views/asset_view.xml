        <record id="view_account_move_asset_filter" model="ir.ui.view">
			<field name="name">Account Asset Report</field>
			<field name="model">account.move</field>
			<field name="arch" type="xml">
				<search string="Search">
                    <field name="date" string="Depreciation Date"/>
                    <field name="ref"/>
                    <field name="depreciation_value"/>
                    <field name="name"/>
                    <field name="asset_depreciated_value"/>
                    <field name="asset_remaining_value"/>
                    <filter string="Branch" name="add_filter" domain="[('branch_id','!=', False)]"/>
                    <filter string="Draft" name="state_by_draft" domain="[('state', '=', 'draft')]"/>
                    <!-- <filter string="Running" name="state_by_running" domain="[('state', '=', 'running')]"/> -->
                    <filter string="Post" name="state_by_post" domain="[('state', '=', 'post')]"/>
                    <group expand='0' string='Group by...'>
                        <filter name="group_by_ref_id" string='Asset' domain="[]" context="{'group_by' : 'ref'}"/>
                        <filter name="group_by_asset_batch_number_id" string='Batch Number' domain="[]" context="{'group_by' : 'asset_batch_number'}"/>
                        <filter name="group_by_asset_parent_id" string='Parent' domain="[]" context="{'group_by' : 'asset_parent_id'}"/>
                        <filter name="group_by_branch_id" string='Branch' domain="[]" context="{'group_by' : 'branch_id'}"/>
                        <filter name="group_by_journal_id" string='Journal' domain="[]" context="{'group_by' : 'journal_id'}"/>
                    </group>
				</search>
			</field>
		</record>

        <record id="view_account_move_asset_tree" model="ir.ui.view">
			<field name="name">Account Asset Report - list</field>
			<field name="model">account.move</field>
			<field name="arch" type="xml">
				<tree string="Depreciation Lines" decoration-info="state == 'draft'" create="0" edit="0" default_order="date asc, id asc" editable="top">
                    <field name="date" string="Depreciation Date" readonly="state in ['cancel', 'posted']"/>
                    <field name="ref" invisible="0"/>
                    <field name="asset_parent_id" invisible="0"/>
                    <field name="reversal_move_id" widget="deprec_lines_reversed"/>
                    <field name="depreciation_value" sum="Total" widget="monetary" string="Depreciation" options="{'currency_field': 'currency_id'}" readonly="state == 'posted'"/>
                    <field name="asset_depreciated_value" sum="Total" readonly="1" force_save="1" options="{'currency_field': 'currency_id'}"/>
                    <field name="asset_remaining_value" sum="Total" readonly="1" widget="monetary" force_save="1" options="{'currency_field': 'currency_id'}"/>
                    <field name="name" readonly="1" string="Entry #"/>
                    <field name="currency_id" column_invisible="False" readonly="state in ['cancel', 'posted']"/>
                    <field name="state" column_invisible="False"/>
                </tree>
			</field>
		</record>

        <!-- Main Action -->
        <record id="menu_action_depreciation_report" model="ir.actions.act_window">
            <field name="name">Depreciation Report</field>
            <field name="res_model">account.move</field>
            <field name="view_mode">tree,form</field>
            <field name="target">current</field>
            <field name="domain">[('asset_id', '!=', False),('state', 'in', ['posted'])]</field>
            <field name="search_view_id" ref="company_memo.view_account_move_asset_filter"/>
            <field name="context">{'search_default_group_by_asset_batch_number_id': 1, 'search_default_group_by_branch_id': 1, 'search_default_group_by_asset_parent_id': 1}</field>

            <!-- <field name="context">{'default_external_memo_request': True, 'default_is_internal_transfer': True,'default_is_fund_transfer': True,'default_payment_type': 'outbound'}</field>-->
        </record>
        <record id="view_depreciation_report_tree_action_bind" model="ir.actions.act_window.view">
            <field name="act_window_id" ref="company_memo.menu_action_depreciation_report"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="company_memo.view_account_move_asset_tree"/>
        </record>

        <!-- Main Action move line-->
        <record id="view_account_move_line_asset_filter" model="ir.ui.view">
			<field name="name">Account Asset Line Report</field>
			<field name="model">account.move.line</field>
			<field name="arch" type="xml">
				<search string="Search">
                    <field name="date" string="Depreciation Date"/>
                    <field name="move_name"/>
                    <field name="account_id"/>
                    <field name="branch_id" string="Business Unit"/>
                    <field name="name" string="Description"/>
                    <field name="debit" string="Debit"/>
                    <field name="credit" string="Credit"/>
                    <field name="asset_id" string="Asset"/>
                    <field name="asset_number" string="Asset #"/>
                    <field name="responsible_bu"/>
                    <field name="asset_batch_number" string="Batch #"/>
                    <filter string="Branch" name="add_filter" domain="[('branch_id','!=', False)]"/>
                    <filter string="Cost center" name="add_cost_center_filter" domain="[('responsible_bu','!=', False)]"/>
                    <!-- <filter string="Draft" name="state_by_draft" domain="[('parent_state', '=', 'draft')]"/>
                    <filter string="Post" name="state_by_post" domain="[('parent_state', '=', 'post')]"/> -->
                    <group expand='0' string='Group by...'>
                        <filter name="group_by_ref_id" string='Asset' domain="[]" context="{'group_by' : 'ref'}"/>
                        <filter name="group_by_asset_batch_number_id" string='Batch Number' domain="[]" context="{'group_by' : 'asset_batch_number'}"/>
                        <filter name="group_by_branch_id" string='Branch' domain="[]" context="{'group_by' : 'branch_id'}"/>
                        <filter name="group_by_responsible_bu" string='Cost Center' domain="[]" context="{'group_by' : 'responsible_bu'}"/>
                        <filter name="group_by_journal_id" string='Journal' domain="[]" context="{'group_by' : 'journal_id'}"/>
                    </group>
				</search>
			</field>
		</record>

        <record id="view_account_move_line_asset_tree" model="ir.ui.view">
			<field name="name">Account Asset Line - list</field>
			<field name="model">account.move.line</field>
			<field name="arch" type="xml">
				<tree string="Depreciation Lines" decoration-info="parent_state == 'draft'" create="0" edit="0" default_order="date asc, id asc">
                    <field name="date" string="Depreciation Date"/>
                    <field name="move_name"/>
                    <field name="asset_id" column_invisible="True"/>
                    <field name="asset_number"/>
                    <field name="asset_batch_number"/>
                    <field name="account_id"/>
                    <field name="branch_id" string="Business Unit"/>
                    <field name="name" string="Description"/>
                    <field name="debit" string="Debit"/>
                    <field name="credit" string="Credit"/>
                    <field name="asset_id" string="Asset"/>
                    <field name="asset_number" string="Asset #"/>
                    <field name="responsible_bu"/>
                    <field name="parent_state" column_invisible="True"/>

                </tree>
			</field>
		</record>
        <record id="menu_action_depreciation_line_report" model="ir.actions.act_window">
            <field name="name">Depreciation Move</field>
            <field name="res_model">account.move.line</field>
            <field name="view_mode">tree,form</field>
            <field name="target">current</field>
            <field name="domain">[('asset_id', '!=', False),('parent_state', 'in', ['posted'])]</field>
