        <record id="memo_asset_model_tree_view" model="ir.ui.view">
            <field name="name">Account Asset Register - list (override)</field>
            <field name="model">account.asset</field>
            <field name="inherit_id" ref="account_asset.view_account_asset_model_tree"/>
            <field name="arch" type="xml">
                <xpath expr="/tree" position="attributes">
                    <attribute name="editable">bottom</attribute>
                    <attribute name="create">0</attribute> 
                </xpath>
                <xpath expr="/tree" position="inside">
                    <header>
                        <button name="generate_child_asset"
                                type="object"
                                class="oe_highlight btn-sm"
                                string="Generate Asset"/>

                         <button name="print_serial_number"
                                type="object"
                                class="oe_highlight btn-sm"
                                string="Print Serial Number"/>

                        <button name="reject_as_nonasset"
                                type="object"
                                class="oe_highlight btn-sm"
                                string="Reject As Non Asset"/>
                    </header>
                </xpath>
                <xpath expr="//field[@name='name']" position="after">
                    <field name="product_desc" readonly="state == 'close'" string="Asset Description"/>
                    <field name="source_location_id" readonly="state == 'close'"/>

                    <field name="serial_number" readonly="state == 'close'"/>
                    <field name="model_id" string="Asset Model" domain="[('state','=', 'model')]" options="{'no_create':'True'}"/>
                    <field name="category_id" options="{'no_create':'True'}" readonly="state == 'close'"/>
                    <field name="branch_id" readonly="state == 'close'"/>
                    <field name="customer_id" readonly="1"/>
                    <field name="responsible_bu" readonly="state == 'close'"/>
                    <field name="previous_location" required="0" readonly="0" force_save="1" invisible="0" string="Previous Location" optional="show"/>
                    <field name="prorata_date" readonly="state == 'close'" string="Start Date"/>
                    <field name="original_value" string="Purchase Value (VAT)"
                        readonly="state not in ['draft','model']"/>

                    <field name="purchase_value_without_tax" 
                        readonly="state not in ['draft','model']" optional="hide"/>
                    <field name="foreign_currency_amount" optional="hide"/> 
                    
                    <field name="inventory_number" readonly="state == 'close'"/>
                    <field name="account_asset_id" readonly="state == 'close'"/>
                    <field name="account_depreciation_id" readonly="state == 'close'"/>
                    <field name="account_depreciation_expense_id"/>
                    <field name="acquisition_date" optional="hide"/>
                    <field name="method"
                        readonly="state not in ['draft','model']"/>
                    <field name="method_number" string="Life (Months)"
                        readonly="state not in ['draft','model']"/>
                    <field name="method_period" string="Period length"
                        readonly="state not in ['draft','model']"/>
                    <field name="company_id" column_invisible="True"
                        readonly="state != 'draft'"/>
                    <field name="state" column_invisible="True"/>
                    <field name="date_of_assignment" optional="hide"/>
                    <field name="date_of_transfer" optional="hide"/>
                    <field name="qty_received" optional="show"/>
                    <field name="is_asset_from_procurement" optional="show" column_invisible="True"/>
                    <field name="depreciation_months_done" optional="show"/>
                    <field name="depreciation_months_remaining" optional="show"/>
                    <field name="depreciation_amount_posted" optional="show"/>
                    <field name="depreciation_amount_remaining" optional="show"/>
                    <field name="depreciation_progress" widget="progressbar" optional="show"/>
                    <button name="view_asset_line" type="object" class="btn-sm" string="View"/>
                </xpath>

            </field>
        </record>

        

