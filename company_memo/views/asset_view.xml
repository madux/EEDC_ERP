        <record id="view_account_asset_search_inherit" model="ir.ui.view">
            <field name="name">account.asset.search.inherit.parent.number</field>
            <field name="model">account.asset</field>
            <field name="inherit_id" ref="account_asset.view_account_asset_search"/>

            <field name="arch" type="xml">

                <!-- Insert into the Filters section -->
                <xpath expr="//field[@name='name']" position="after">
                    <field name="parent_number"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    
                    <!-- Normal Filter -->
                    <filter name="filter_parent_number"
                            string="Parent Number Set"
                            domain="[('parent_number', '!=', False)]"/>

                    <!-- Group By Section -->
                    <group expand="0" string="Group By">
                        <filter name="group_parent_number"
                                string="Parent Number"
                                context="{'group_by': 'parent_number'}"/>
                    </group>

                </xpath>

            </field>
        </record>

